@{
    ViewData["Title"] = "OwnerRegister";
  
}


<form id="ownerForm" asp-action="Create" asp-controller="OwnerController">
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Name</label>
        <input type="email" class="form-control" id="Name" aria-describedby="emailHelp">
        <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Telephone</label>
        <input type="password" class="form-control" id="Telephone">
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Email</label>
        <input type="email" class="form-control" id="Email" aria-describedby="emailHelp">
        <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
    </div>
        <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Identification Number</label>
        <input type="password" class="form-control" id="IdentificationNumber">
    </div
        <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Address</label>
        <input type="password" class="form-control" id="Address">
    </div
    <button type="submit" class="btn btn-primary">Submit</button>
</form>